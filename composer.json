{
    "name": "toxicity/altered-api-php",
    "type": "library",
    "description": "Eases the api usage from the altered.gg website.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Toxicity\\AlteredApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Toxicity\\AlteredApi\\tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "John Quairia",
            "email": "tech@toxicity.be"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:toxicity1985/atoum.git"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "^7.3",
        "symfony/http-client": "^7.3",
        "symfony/validator": "^7.3",
        "doctrine/orm": "^3",
        "doctrine/dbal": "^4",
        "symfony/cache": "^7.3",
        "symfony/filesystem": "^7.3",
        "symfony/rate-limiter": "^7.3"
    },
    "require-dev": {
        "symfony/panther": "^2.2",
        "atoum/atoum": "dev-update/php8.4"
    },
    "scripts": {
        "test": "vendor/bin/atoum -c .atoum.php",
        "test-coverage": "vendor/bin/atoum -c .atoum.php --enable-branch-and-path-coverage"
    }
}
